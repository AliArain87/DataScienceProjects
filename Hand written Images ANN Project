      "source": [
        "from tensorflow.keras import Sequential\n",
        "from tensorflow.keras.layers import Dense\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "_WqOq2p_eros"
      },
      "outputs": [],
      "source": [
        "# Now here we defining our model\n",
        "model=Sequential([\n",
        "                  # First Hidden Layer\n",
        "                  Dense(units=10,\n",
        "                        input_shape=(784,),\n",
        "                        activation='sigmoid')\n",
        "                  Dense(units=10,\n",
        "                        activation='sigmoid')\n",
        "                  \n",
        "])\n",
        "# Compiling the Model\n",
        "model.compile(\n",
        "    optimizer='adam',\n",
        "    loss='sparse_categorical_crossentropy',\n",
        "    metrics=['accuracy']\n",
        "\n",
        ")"
      ]
    },
